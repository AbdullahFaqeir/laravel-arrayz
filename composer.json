{
  "name": "alfaqeir/laravel-arrayz",
  "description": "A hub from giriannamalai/Arrayz to laravel",
  "authors": [
    {
      "name": "Abdullah Al-Faqeir",
      "email": "abdullah@devloops.net"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "alfaqeir/laravel-arrayz": "1.0.0"
  },
  "require-dev": {
    "alfaqeir/laravel-arrayz": "1.0.0"
  },
  "autoload": {
    "psr-4": {
      "AlFaqeir\\Arrayz\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlFaqeir\\Arrayz\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "AlFaqeir\\Arrayz\\ServiceProvider\\ArrayzServiceProvider"
      ],
      "aliases": {
        "Arrayz": "AlFaqeir\\Arrayz\\Facades\\ArrayzFacades"
      }
    }
  }
}
